{% extends 'baseadmin.html.twig' %}


{% block title %}Config{% endblock %}

{% block body %}
    <h1>Config</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ config.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ config.name }}</td>
            </tr>
            <tr>
                <th>Slogan</th>
                <td>{{ config.slogan }}</td>
            </tr>
            <tr>
                <th>Banner</th>
                <td>{{ config.banner }}</td>
            </tr>
            <tr>
                <th>About</th>
                <td>{{ config.about }}</td>
            </tr>
            <tr>
                <th>Contact</th>
                <td>{{ config.contact }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ config.title }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_config_index') }}">back to list</a>

    <a href="{{ path('app_config_edit', {'id': config.id}) }}">edit</a>

    {{ include('admin/config/_delete_form.html.twig') }}
{% endblock %}
